<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="mbRadarView">
        <attr name="linecolor" format="color"/>
        <attr name="contentColor" format="color"/>
        <attr name="side" format="dimension"/>
        <attr name="distance" format="dimension"/>
        <attr name="number" format="integer"/>

    </declare-styleable>


    <declare-styleable name="MyProgressLine">
        <attr name="strokeColor" format="color"/>
        <attr name="fillColor" format="color"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="fillHeight" format="dimension"/>
        <attr name="maxProgress" format="integer"/>
        <attr name="progressNumber" format="integer"/>

    </declare-styleable>

</resources>